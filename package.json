{
  "name": "vport-app",
  "version": "1.0.0",
  "description": "vport API",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./index.js",
    "start:dev": "nodemon --watch ./src --exec babel-node ./index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate:undo && npm run migrate && npm run seed",
    "test": "node_env=test nyc --reporter=lcov --reporter=text mocha --require babel-polyfill --require babel-register ./src/tests/*/**.js"
  },
  "author": "vport NG team",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "socket.io": "^2.1.1",
    "supertest": "^3.1.0"
  }
}
